<% include ./head %>
<br><br><br>
<table class="table table-striped table-bordered">
    <thead class="thead-dark">
        <th>Priority</th>
        <th>Institute ID</th>
        <th>Program Name</th>
        <th>#</th>
    </thead>
    <tbody>
        <% for(let i=0;i<programs.length;i++){ %>
            <tr>
                <td scope="row"><%= programs[i].Priority %></td>
                <td><%= programs[i].Name %></td>
                <td><%= programs[i].ProgramName %></td>
                <td><form action='/ProgramChosen/<%=programs[i].InstituteId%>/<%=programs[i].ProgramName%>/<%=programs[i].StudentRollNumber%>/<%=programs[i].Priority%>?_method=DELETE' method='POST'><button type="submit" class="btn btn-danger btn-block">Delete</button></form></td>
            </tr>
        <% } %>
    </tbody>
</table>
<br><br><br><br><hr class="display-2">
<a href="/register" class="btn btn-primary btn-lg float-center">Add Choices Your Form</a>

<% include ./tail %>